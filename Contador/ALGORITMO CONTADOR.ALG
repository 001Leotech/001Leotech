Algoritmo "Algoritmo contador"

Var
   Contador, Valor, Salto: Inteiro


Inicio
        Contador <- 0
     Escreval("Até que valor quer contar? ")
     Leia(Valor)
     Escreval("De quanto você quer o salto? ")
     Leia(Salto)
     Enquanto (Contador <= Valor) faca
     Escreval(Contador)
     Contador <- Contador + Salto
     Fimenquanto
     Escreval("Terminei de contar")

Fimalgoritmo