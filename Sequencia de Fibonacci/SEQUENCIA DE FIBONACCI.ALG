Algoritmo "SEQUENCIA DE FIBONACCI"
Var
     C, N1, N2, N3: Inteiro


Inicio
     EscrevaL("- - - LB SOLUÇÕES - - - ")
     Escreval(" ")
     Escreval(" SEQUENCIA DE FIBONACCI ")
     Escreval(" ")

      N1 <- 0
      Escreva(N1)
      N2 <- 1
      Escreva(N2)
      Para C de 1 ate 13 faca
      N3 <- N1 + N2
      Escreva(N3)
      N1 <- N2
      N2 <- N3
      Fimpara


Fimalgoritmo